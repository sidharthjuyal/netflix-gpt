  "dependencies": {
    "@parcel/core": "^2.15.2",
    "@reduxjs/toolkit": "^2.8.2",
    "@tailwindcss/postcss": "^4.1.10",
    "firebase": "^11.9.1",
    "groq-sdk": "^0.25.0",
    "node": "^24.2.0",
